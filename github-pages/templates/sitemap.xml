<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
  <url>
    <loc>
    http://${user}.github.io/
    </loc>
    <changefreq>weekly</changefreq>
    <lastmod>$(date)</lastmod>
    <priority>1.0</priority>
    <image:image>
    <image:loc>
    https://${user}.github.io/images/github.png
    </image:loc>
    <image:caption>${User} GitHub Pages </image:caption>
    <image:title>${Name} GitHub Pages </image:title>
    </image:image>
  </url>
</urlset>
